{
  "name": "fckai",
  "displayName": "f-ckAI",
  "description": "Injects funny, sarcastic roast comments into your code using AI APIs like OpenAI, Groq, or Gemini.",
  "version": "0.0.1",
  "type": "module",
  "publisher": "FckingAIs069",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "roast",
    "funny",
    "code comments",
    "openai",
    "groq",
    "gemini"
  ],
  "activationEvents": [
    "onCommand:fckai.addFunnyComments",
    "onCommand:fckai.manageApiKeys"
  ],
  "main": "./out/extension.js",
  "icon": "accessory.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rohit-554/f-ckAI"
  },
  "contributes": {
    "configuration": {
      "title": "F*ck AI - Roast Plugin",
      "properties": {
        "fckai.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your API key for OpenAI, Groq, or Gemini. (No, I wonâ€™t use it to buy coffee.)"
        },
        "fckai.apiProvider": {
          "type": "string",
          "enum": [
            "openai",
            "groq",
            "gemini"
          ],
          "default": "openai",
          "description": "Choose your poison. Which AI shall we roast today?"
        }
      }
    },
    "commands": [
      {
        "command": "fckai.addFunnyComments",
        "title": "Add Funny AI Comments",
        "category": "FCK AI"
      },
      {
        "command": "fckai.manageApiKeys",
        "title": "Manage API Keys",
        "category": "FCK AI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
